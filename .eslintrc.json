{
  "extends": ["eslint:recommended", "next"],
  "rules": {
    "no-undef": "off",
    "no-empty-pattern": ["error", { "allowObjectPatternsAsParameters": true }],
    "no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^...|_"
      }
    ],
    "react-hooks/exhaustive-deps": "off"
  }
}
